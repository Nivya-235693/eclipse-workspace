package UstDemo;

public class arithmetic {

	public static void main(String[] args) {
		int i=10;
		int j=5;
		int k=i+j;
		int l=i-j;
		int m=i*j;
		int n=i/j;
		System.out.println("The sum of two number is : "+k);
		System.out.println("The difference of two number is : "+l);
		System.out.println("The product of two number is : "+m);
		System.out.println("The quotient of two number is : "+n);
	}
}